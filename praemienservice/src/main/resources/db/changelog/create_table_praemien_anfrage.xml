<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property name="uuid_type" value="uuid" dbms="postgresql"/>

    <changeSet id="create_table_praemien_anfrage" author="michael">
        <createTable tableName="PRAEMIEN_ANFRAGE" schemaName="praemien_service">
            <column name="praemien_id" type="${uuid_type}">
                <constraints primaryKey="true" nullable="false" primaryKeyName="PRAEMIEN_ANFRAGE_PK"/>
            </column>
            <column name="kilometerleistung" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="fahrzeugtyp" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="bundesland" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="kreis" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="stadt" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="postleitzahl" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="bezirk" type="text">
                <constraints nullable="true"/>
            </column>
            <column name="praemie" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
